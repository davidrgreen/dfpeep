{
	"name": "DFPeep",
	"manifest_version": 2,
	"version": "0.1",
	"description": "Analyze ads served from DFP.",
	"devtools_page": "devtools/devtools.html",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["page/insert-scripts.js"],
			"css": ["page/page.css"],
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [
		"page/collect-data.js",
		"page/page.css"
	],
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
